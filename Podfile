# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Esqueleto' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Esqueleto
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Core'

  target 'EsqueletoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Firebase/Core'
  end

  target 'EsqueletoUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Firebase/Core'
  end

end
